<div class="container p-3" style="margin-top: 2em">
  <div class="row">
    <div class="col-6">
      <h5 class="fw-bold">Halaman Panel Admin</h5>
      <p>Cari data terkait</p>
      <form action="">
        <input type="text" class="form-control" />
      </form>
    </div>
  </div>

  <table class="table mt-3">
    <thead class="table-success">
      <tr>
        <th scope="col">No</th>
        <th scope="col">Nama</th>
        <th scope="col">Email</th>
        <th scope="col">Nomor Hp</th>
        <th scope="col">Jenis Kelamin</th>
        <th scope="col">Alamat</th>
        <th scope="col">Aksi</th>
      </tr>
    </thead>
    <tbody>
      <% i=1 %> <% result.forEach((datas) => { %>
      <tr>
        <td><%= i++ %></td>
        <td><%= datas.nama %></td>
        <td><%= datas.email %></td>
        <td><%= datas.nomor_hp %></td>
        <td><%= datas.jenis_kelamin %></td>
        <td><%= datas.alamat %></td>
        <td>
          <a href="">
            <button class="btn btn-danger badge">Hapus Data</button>
          </a>
          <br />
          <a href="">
            <button class="btn btn-warning badge">Edit Data</button>
          </a>
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
